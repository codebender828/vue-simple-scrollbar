{
  "name": "vue-simple-scrollbar",
  "version": "0.1.1",
  "description": "A light-weight customizable Vue scrollbar component portal for `simple-scrollbar`",
  "author": {
    "name": "Jonathan Bakebwa",
    "url": "https://github.com/codebender828"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codebender828/vue-simple-scrollbar"
  },
  "main": "dist/vue-simple-scrollbar.umd.js",
  "module": "dist/vue-simple-scrollbar.esm.js",
  "unpkg": "dist/vue-simple-scrollbar.min.js",
  "browser": {
    "./sfc": "src/components/vue-simple-scrollbar.vue"
  },
  "keywords": [
    "vue",
    "parrots",
    "scrollbar",
    "simple-scrollbar",
    "vue-simple-scrollbar"
  ],
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:npm": "vue-cli-service build --target lib --name vue-simple-scrollbar src/components/vue-simple-scrollbar.vue",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:coverage": "yarn test:unit -- --coverage",
    "test:badges": "yarn test:coverage  && jest-coverage-badges"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "jest-coverage-badges": "^1.1.2",
    "nprogress": "^0.2.0",
    "simple-scrollbar": "^0.4.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vue-simple-scrollbar": "^0.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-unit-jest": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  }
}
